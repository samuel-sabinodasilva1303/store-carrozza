.page-comparador
  .main__container > .board
    width 100%
    min-height 90rem
    padding-top 30px

  .board
    > h1.color
      font-family: 'Public Sans';
      font-style: normal;
      font-weight: 700;
      font-size: 24px;
      line-height: 130%;
      color: #292F3E;

    > .comparatorTabs, > .left
      display none

    > .comparatorTabs, > .right
      display none

    > .comparator
      width 100%
      height 100%
      display flex
      padding-top 24px
      position relative
      align-items flex-end

      > ul
        width 100%
        height 100%
        display flex
        margin-bottom 50px
        align-items center

        > li.Labels
          display none

        > li
          border 1px solid #D6DBE3
          margin 0.4rem 0.2rem 
          background white
          width 408px
          height 158px
          border-radius 5px
          position relative
          display flex
          margin-bottom 50px

          > ul
            width 100%
            height 100%
            padding 10px
            display flex
            position relative
            justify-content center
            align-items center
            
            .comparsionFoto
              display flex
              position absolute
              width 95%
              height 92%
              flex-direction column
              justify-content center
             
              > a
                width 97%
                height 97%
                display flex
                font-family 'Public Sans'
                font-style normal
                font-weight 500
                font-size 12px
                line-height 130%
                color #515970
                position absolute
                top 0
                left 0

                img
                  width 11em
                  object-fit contain
                  margin-right 0.3rem

              > div
                position absolute
                width 100%
                height 100%

                > a
                  display flex
                  margin 0
                  img
                    display none
                  
                  &:after
                    content "Comprar"
                    display flex
                    flex-direction row
                    justify-content center
                    padding 10px 25px
                    position absolute
                    width: 131px;
                    height: 38px;
                    left: calc(97% - 62%);
                    top 110px
                    background #000000
                    border-radius 8px
                    color #fff
                    font-family 'Public Sans'
                    font-style normal
                    font-weight 700
                    font-size 14px
                    line-height 130%
                    align-items center
                    margin: -13px 2px;

              .closeComp
                display flex
                width 24px
                height 24px
                position absolute
                transform: translate(1520%,20%);
                background-image: url("data:image/svg+xml;base64,")
                background-size cover
                color transparent

